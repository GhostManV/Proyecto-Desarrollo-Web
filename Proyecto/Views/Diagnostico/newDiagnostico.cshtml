@model WebApplication1.Models.Diagnosticos
@{
    ViewBag.Title = "newDiagnostico";
}

<h2>@ViewBag.Title</h2>

@using (Html.BeginForm("Action", "Controller", FormMethod.Post, new { role = "form", @class = "" }))
{
    <div class="mb-3 mt-3">
        @Html.LabelFor(m => m.titulo)
        @Html.TextBoxFor(m => m.titulo, new { @class = "form-control" })
    </div>

    <div class="mb-3 mt-3">
        @Html.LabelFor(m => m.descripcion)
        @Html.TextBoxFor(m => m.descripcion, new { @class = "form-control" })
    </div>

    <button type="submit" class="btn btn-primary">Agregar Descripcion</button>
}